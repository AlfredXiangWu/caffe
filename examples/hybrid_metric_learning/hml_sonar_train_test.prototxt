   	param: "conv10_w"
	param: "conv10_b" 
}
layers{
  name: "fc1"
  type: INNER_PRODUCT
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  inner_product_param {
	num_output: 128
	weight_filler {
	  type: "xavier"
	}
	bias_filler {
	  type: "constant"
	  value: 0.1
	}	
  }  
  bottom: "conv10"
  top: "fc1"
   	param: "fc1_w"
	param: "fc1_b"  
}

layers{
  name: "relu10"
  type: RELU
  bottom: "fc1"
  top: "fc1"
}

layers{
  name: "dropout1"
  type: DROPOUT
  dropout_param{
	dropout_ratio:0.9
  }
  bottom: "fc1"
  top: "dropout1"
}
layers{
  name: "fc2"
  type: INNER_PRODUCT
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  inner_product_param{
	num_output: 700
	weight_filler {
	  type: "xavier"
	}
	bias_filler {
	  type: "constant"
	  value: 0.1
	}	
  }
  bottom: "dropout1"
  top: "fc2"
}

layers {
  name: "softmax"
  type: SOFTMAX
  bottom: "fc2"
  top: "prob"
  include: { phase: TEST }
}

layers {
  name: "accuracy"
  type: ACCURACY
  bottom: "prob"
  bottom: "label"
  top: "accuracy"
  include: { phase: TEST }
}


layers{
  name: "loss"
  type: SOFTMAX_LOSS
  bottom: "fc2"
  bottom: "label"
  loss_weight: 0.5  
}



