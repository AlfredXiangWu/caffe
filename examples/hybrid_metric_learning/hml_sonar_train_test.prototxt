	}
  }
  bottom: "conv7"
  top: "conv8"
   	param: "conv8_w"
	param: "conv8_b" 
}
layers{
  name: "pool3"
  type: POOLING
  pooling_param {
	pool: MAX
	kernel_size: 2
	stride: 2
  }
  bottom: "conv8"
  top: "pool3"
}
layers{
  name: "conv9"
  type: CONVOLUTION
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  convolution_param{
    num_output: 128
    kernel_size: 3
    stride: 1
    weight_filler{
      type:"xavier"
    }
    bias_filler{
      type: "constant"
      value: 0.1
    
    }
  }
  bottom: "pool3"
  top: "conv9"
   	param: "conv9_w"
	param: "conv9_b" 
}
layers{
  name: "conv10"
  type: CONVOLUTION
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  convolution_param {
	num_output: 128
	kernel_size: 1
	stride: 1
	weight_filler {
	  type: "xavier"
	}
	bias_filler {
