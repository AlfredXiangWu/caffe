name: "hml_sonar_net"
layers {
  name: "data"
  type: PAIR_DATA
  top: "data"
  top: "label"
  top: "data2"
  top: "label2"
  pair_data_param{
	  source: "../../data/UCI/sonar/sonar_train_lmdb"
	  backend: LMDB
	  source_list: "../../data/lmdb/train_rng7610_linux.lab"
	  pairs_list: "../../data/lmdb/MBGC700_train_pairs.txt"
	  batch_size: 64
	}
  transform_param {
    scale: 0.00390625
    crop_size: 64
    mirror: true
  }
  include: { phase: TRAIN }
}

layers {
  name: "data"
  type: PAIR_DATA
  top: "data"
  top: "label"
  top: "data2"
  top: "label2"
  pair_data_param{
	  source: "../../data/lmdb/MBGC-val-0.0.0.9_lmdb"
	  backend: LMDB
	  source_list: "../../data/lmdb/val_linux.lab"
	  pairs_list: "../../data/lmdb/MBGC700_val_pairs.txt"	  
	  batch_size: 16
	}
  transform_param {
    scale: 0.00390625
    crop_size: 64
    mirror: true
  }
  include: { phase: TEST }
}

layers{
  name: "conv1"
  type: CONVOLUTION
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  convolution_param {
	num_output: 32
	kernel_size: 3
	stride: 1
	weight_filler {
	  type: "xavier"
	}
	bias_filler {
	  type: "constant"
	  value: 0.1
	}
  }
  bottom: "data"
  top: "conv1"
   	param: "conv1_w"
	param: "conv1_b" 
}
layers {
  name: "relu1"
  type: RELU
  bottom: "conv1"
  top: "conv1"
}
layers{
  name: "conv2"
  type: CONVOLUTION
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  convolution_param {
	num_output: 32
	kernel_size: 3
	stride: 1
	weight_filler {
	  type: "xavier"
	}
	bias_filler {
	  type: "constant"
	  value: 0.1
	}
  }
  bottom: "conv1"
  top: "conv2"
   	param: "conv2_w"
	param: "conv2_b" 
}
layers {
  name: "relu2"
  type: RELU
  bottom: "conv2"
  top: "conv2"
}

layers{
  name: "pool1"
  type: POOLING
  pooling_param {
	pool: MAX
	kernel_size: 2
	stride: 2
  }
  bottom: "conv2"
  top: "pool1"
}
layers{
  name: "conv3"
